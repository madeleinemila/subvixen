

$(document).ready(function() {

    $('#play').on('click', function() {
      video.play();
    });

    $('#pause').on('click', function() {
      video.pause();
    });

    $('#loop').on('click', function() {
      video.loop ? video.loop = false : video.loop = true;
      $(this).toggleClass( "on" );
    });


});
